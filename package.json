  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/cookie": "^0.3.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/validator": "^9.4.1",
    "dotenv": "^6.0.0",
    "graphql-cli": "^2.16.5",
    "nodemon": "^1.18.3",
